{
    "manifest_version": 2,
    "name": "TasteMaker",
    "version": "0.0.3",
    "options_page": "options.html",
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon-34.png"
    },
    "icons": {
        "128": "icon-128.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["contentScript.bundle.js"],
        "css": ["content.styles.css"]
    }],
    "web_accegit sible_resources": [
        "content.styles.css",
        "icon-128.png",
        "icon-34.png",
        "assets/img/*.png"
    ],
    "content_security_policy": "script-src 'self' https://www.google-analytics.com/analytics.js 'sha256-tqBkxcARnGE+nMHDdA6LouxcAYCpa9mPuMHntrOa6o0=' 'unsafe-inline' 'unsafe-eval' ; object-src 'self'",
    "permissions": [
        "cookies",
        "*://*.google.com",
        "tabs",
        "storage",
        "identity",
        "https://www.facebook.com/",
        "https://graph.facebook.com/"
    ]
}
